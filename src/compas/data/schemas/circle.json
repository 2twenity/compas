{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "circle.json",
    "$compas": "1.6.3",
    "type": "object",
    "properties": {
        "plane": {
            "type": "array",
            "minItems": 2,
            "maxItems": 2,
            "items": [
                {
                    "$ref": "compas.json#/definitions/point"
                },
                {
                    "$ref": "compas.json#/definitions/vector"
                }
            ]
        },
        "radius": {
            "type": "number",
            "exclusiveMinimum": 0
        }
    },
    "required": [
        "plane",
        "radius"
    ]
}
